{
  "name": "@all-user/ok-patterns-lines",
  "version": "0.1.0",
  "description": "Patterns Plugin for OK BLOCKS.",
  "main": "lib/index.js",
  "scripts": {
    "help": "npm-run-help",
    "install": "run-s install:*",
    "install:types": "if [ -L node_modules/@all-user/ok-patterns-lines.types ]; then :; else npm run setup:types; fi",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "setup": "run-s setup:*",
    "setup:types": "rm -rf node_modules/@all-user/ok-patterns-lines.types && mkdirp node_modules/@all-user && ln -fs `pwd`/packages/@all-user/ok-patterns-lines.types node_modules/@all-user/ok-patterns-lines.types",
    "lint": "run-p lint:*",
    "lint:src": "eslint src/js/**/*.js",
    "lint:test": "eslint test/**/*.test.js",
    "build:js": "babel src/js -x .js -d lib/js",
    "watch:js": "babel -w src/js -x .js -d lib/js",
    "prepublish": "run-s build release",
    "build:css": "stylus -u nib -c ./src/stylus/index.styl -o ./src/bundle.css -m",
    "watch:css": "stylus -u nib -c -w ./src/stylus/index.styl -o ./src/bundle.css -m",
    "release:css": "stylus -u nib -c ./src/stylus/index.styl -o ./dist/bundle.css",
    "release:js": "browserify -t babelify -r ./src/js/index.js:@all-user/ok-patterns-lines | uglifyjs > ./dist/bundle.js",
    "release:clean": "rimraf ./dist && mkdirp ./dist",
    "release": "run-s release:clean release:*",
    "build": "run-s build:*",
    "watch": "run-p watch:*",
    "bumpup": "bin/bumpup '@all-user/ok-patterns-lines.types'",
    "start": "npm run watch"
  },
  "author": "Keita Okamoto <ok@aaa.email.ne.jp>",
  "license": "MIT",
  "devDependencies": {
    "append-css": "^0.1.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.6",
    "babel-preset-env": "^1.3.3",
    "babel-preset-flow": "^6.23.0",
    "cpx": "^1.2.1",
    "eslint": "^1.10.3",
    "eslint-plugin-flowtype": "^2.30.4",
    "exorcist": "^0.4.0",
    "flow-bin": "^0.43.1",
    "flow-typed": "^2.0.0",
    "mkdirp": "*",
    "mocha": "^2.2.5",
    "nib": "^1.1.0",
    "npm-run-all": "^4.0.2",
    "npm-run-help": "^1.1.0",
    "power-assert": "^1.2.0",
    "rimraf": "^2.4.3",
    "stylus": "^0.52.0",
    "uglify-js": "^2.4.24",
    "watchify": "^3.3.1"
  },
  "peerDependencies": {
    "@all-user/ok-blocks": "^0.0.7"
  },
  "dependencies": {}
}
